{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AMusic",
  "version": "0.1.0",
  "identifier": "com.amusic.app",
  "build": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "AMusic",
        "width": 1100,
        "height": 600,
        "resizable": true,
        "fullscreen": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "appimage",
    "linux": {
      "appimage": {
        "bundleMediaFramework": false,
        "files": {}
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
